<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE log4j:configuration SYSTEM "log4j.dtd">

<log4j:configuration xmlns:log4j="http://jakarta.apache.org/log4j/" debug="false">

  <appender name="Console" class="org.apache.log4j.ConsoleAppender">
    <layout class="org.apache.log4j.PatternLayout">
      <param name="ConversionPattern" value="%d\t%p\t%c\t%m%n"/>
    </layout>
  </appender>

  <logger name="ca.bc.gov.bcdem">
    <level value="INFO"/>
  </logger>

  <logger name="com.revolsys.gis.tools">
    <level value="INFO"/>
  </logger>

  <logger name="com.revolsys.gis.parallel">
    <level value="INFO"/>
  </logger>

  <logger name="com.revolsys.parallel.tools">
    <level value="INFO"/>
  </logger>
  
  <logger name="com.revolsys.gis.io.LabelCountMap">
    <level value="INFO"/>
  </logger>
   
  <logger name="org.docx4j.model.properties">
    <level value="FATAL"/>
  </logger>
    
  <logger name="org.docx4j">
    <level value="FATAL"/>
  </logger>
  
  <root >
    <level value="ERROR" />
    <appender-ref ref="Console"/>
  </root>
</log4j:configuration>
